--find power of number
import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int n=sc.nextInt();
        int power=1;
        for(int i=1;i<=n;i++){
            power*=x;
        }
        System.out.println(power);
    }
}
